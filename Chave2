    Name = "Lançar Jogador Mais Próximo para o Ar",
    Flag = "Toggle3",
    Callback = function(state)
        if state then
            LaunchClosestPlayer()
        end
    end
})

PlayersFunctionsTab:CreateToggle({
    Name = "Diminuir Velocidade do Jogador Mais Próximo",
    Flag = "Toggle4",
    Callback = function(state)
        if state then
            SlowDownClosestPlayer()
        else
            -- Restaurar velocidade do jogador
            local targetPlayer = GetVisibleClosestPlayer()
            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
                targetPlayer.Character.Humanoid.WalkSpeed = 16
                print("Velocidade do jogador " .. targetPlayer.Name .. " restaurada")
            end
        end
    end
})

PlayersFunctionsTab:CreateToggle({
    Name = "Desarmar Jogador Mais Próximo",
    Flag = "Toggle5",
    Callback = function(state)
        if state then
            DisarmClosestPlayer()
        end
    end
})

