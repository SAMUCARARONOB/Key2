local PlayersFunctionsTab = Window:CreateTab("PLAYERS FUNCTIONS", 6031280883)
local PlayersFunctionsSection = PlayersFunctionsTab:CreateSection("Funções dos Jogadores")
local PlayersFunctionsLabel = PlayersFunctionsTab:CreateLabel("Adicione aqui suas funções personalizadas para jogadores!")

-- Função para verificar se a cabeça do jogador alvo está visível
local function IsHeadVisible(targetPlayer)
    local camera = game.Workspace.CurrentCamera
    local headPosition = targetPlayer.Character.Head.Position
    local ray = Ray.new(camera.CFrame.Position, (headPosition - camera.CFrame.Position).unit * 500)
    local part = workspace:FindPartOnRayWithIgnoreList(ray, {game.Players.LocalPlayer.Character, targetPlayer.Character})
    
    return not part
end

-- Função para encontrar o jogador visível mais próximo
local function GetVisibleClosestPlayer()
    local visibleClosestPlayer = nil
    local closestDistance = math.huge
    local localPlayer = game.Players.LocalPlayer
    local localHumanoidRootPart = localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart")

    if not localHumanoidRootPart then return end

    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= localPlayer and player.Character and player.Character:FindFirstChild("Head") and IsHeadVisible(player) then
            local targetHumanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
            if targetHumanoidRootPart then
                local distance = (localHumanoidRootPart.Position - targetHumanoidRootPart.Position).Magnitude
                if distance < closestDistance then
                    visibleClosestPlayer = player
                    closestDistance = distance
                end
            end
        end
    end
    return visibleClosestPlayer
end

-- Função de teletransporte para o jogador mais próximo
local function TeleportToClosestPlayer()
    local targetPlayer = GetVisibleClosestPlayer()
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if humanoidRootPart then
            humanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
            print("Teletransportado para " .. targetPlayer.Name)
        else
            print("Não foi possível encontrar a HumanoidRootPart do jogador local")
        end
    else
        print("Jogador visível mais próximo não encontrado ou sem HumanoidRootPart")
    end
end

-- Função para congelar o jogador mais próximo
local function FreezeClosestPlayer()
    local targetPlayer = GetVisibleClosestPlayer()
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
        targetPlayer.Character.Humanoid.WalkSpeed = 0
        print("Jogador " .. targetPlayer.Name .. " congelado")
    end
end

-- Função para lançar o jogador mais próximo para o ar
local function LaunchClosestPlayer()
    local targetPlayer = GetVisibleClosestPlayer()
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        targetPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 100, 0)
        print("Jogador " .. targetPlayer.Name .. " lançado para o ar")
    end
end

-- Função para diminuir a velocidade do jogador mais próximo
local function SlowDownClosestPlayer()
    local targetPlayer = GetVisibleClosestPlayer()
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Humanoid") then
        targetPlayer.Character.Humanoid.WalkSpeed = 8
        print("Velocidade do jogador " .. targetPlayer.Name .. " reduzida")
    end
end

-- Função para desarmar o jogador mais próximo
local function DisarmClosestPlayer()
    local targetPlayer = GetVisibleClosestPlayer()
    if targetPlayer and targetPlayer.Character then
        for _, tool in pairs(targetPlayer.Backpack:GetChildren()) do
            if tool:IsA("Tool") then
                tool:Destroy()
                print("Jogador " .. targetPlayer.Name .. " desarmado")
            end
        end
        for _, tool in pairs(targetPlayer.Character:GetChildren()) do
            if tool:IsA("Tool") then
                tool:Destroy()
                print("Jogador " .. targetPlayer.Name .. " desarmado")
            end
        end
    end
end

-- Função para ativar o aimbot
local aimbotConnection
local function EnableAimbot()
    aimbotConnection = game:GetService("RunService").RenderStepped:Connect(function()
        local targetPlayer = GetVisibleClosestPlayer()
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
            local headPosition = targetPlayer.Character.Head.Position
            local camera = game.Workspace.CurrentCamera
            camera.CFrame = CFrame.new(camera.CFrame.Position, headPosition)
        end
    end)
end

local function DisableAimbot()
    if aimbotConnection then
        aimbotConnection:Disconnect()
        aimbotConnection = nil
    end
end

-- Função para aumentar o dano causado
local function IncreaseDamage()
    local player = game.Players.LocalPlayer
    for _, tool in pairs(player.Backpack:GetChildren()) do
        if tool:IsA("Tool") and tool:FindFirstChild("Handle") then
            local damageScript = tool:FindFirstChildOfClass("Script")
            if damageScript then
                damageScript.Disabled = true
                -- Script customizado de aumento de dano
                local newScript = Instance.new("Script")
                newScript.Source = [[
                    local tool = script.Parent
                    tool.Handle.Touched:Connect(function(hit)
                        local character = hit.Parent
                        local humanoid = character:FindFirstChildOfClass("Humanoid")
                        if humanoid then
                            humanoid:TakeDamage(50) -- Dano aumentado
                        end
                    end)
                ]]
                newScript.Parent = tool
            end
        end
    end
    print("Dano aumentado")
end

-- Função para aumentar a saúde
local function IncreaseHealth()
    local player = game.Players.LocalPlayer
    player.Character.Humanoid.MaxHealth = player.Character.Humanoid.MaxHealth * 2
    player.Character.Humanoid.Health = player.Character.Humanoid.MaxHealth
    print("Saúde aumentada")
end

-- Função para super salto
local function SuperJump()
    local player = game.Players.LocalPlayer
    player.Character.Humanoid.JumpPower = 100
    print("Super salto ativado")
end

-- Função para invisibilidade
local function BecomeInvisible()
    local player = game.Players.LocalPlayer
    for _, part in pairs(player.Character:GetChildren()) do
        if part:IsA("BasePart") then
            part.Transparency = 1
            part.CanCollide = false
        end
    end
    print("Invisibilidade ativada")
end

-- Função para NoClip
local function EnableNoClip()
    local player = game.Players.LocalPlayer
    local noClipConnection
    noClipConnection = game:GetService("RunService").Stepped:Connect(function()
        for _, part in pairs(player.Character:GetChildren()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end)
    print("NoClip ativado")
end

